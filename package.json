{
  "name": "scrcpy-gui",
  "version": "2026.1.21",
  "description": "Windows GUI for Scrcpy",
  "author": "KB",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --windows"
  },
  "build": {
    "appId": "com.scrcpy.gui",
    "productName": "Scrcpy GUI",
    "win": {
      "icon": "build/icon.ico",
      "target": [
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "directories": {
      "output": "dist"
    }
  },
  "devDependencies": {
    "electron": "^40.0.0",
    "electron-builder": "^26.5.0"
  }
}
